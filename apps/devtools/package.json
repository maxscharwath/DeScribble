{
  "name": "devtools",
  "version": "0.0.0",
  "displayName": "Condensation Devtools",
  "author": "Maxime Scharwath <maxime@scharwath.ch>",
  "description": "A devtools extension for Condensation DS",
  "scripts": {
    "build": "vite build",
    "watch": "vite build --watch --mode development --minify false",
    "dev": "vite",
    "format": "eslint --ext .js,.jsx,.ts,.tsx,.vue src --fix",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx,.vue src",
    "serve:firefox": "web-ext run --start-url \"about:debugging#/runtime/this-firefox\"  --source-dir ./dist/",
    "serve:chrome": "web-ext run -t chromium --start-url \"https://google.com\" --source-dir ./dist/"
  },
  "license": "MIT",
  "devDependencies": {
    "@samrum/vite-plugin-web-extension": "^4.1.0",
    "@types/chrome": "^0.0.233",
    "@types/dagre": "^0.7.48",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.1",
    "@types/webextension-polyfill": "^0.10.0",
    "@vitejs/plugin-react": "^4.0.0",
    "eslint-config-custom": "workspace:*",
    "tsconfig": "workspace:*",
    "typescript": "^5.0.4",
    "vite": "~4.3.3",
    "web-ext": "^7.6.2",
    "web-worker": "^1.2.0"
  },
  "dependencies": {
    "dagre": "^0.8.5",
    "elkjs": "^0.8.2",
    "object-hash": "^3.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "reactflow": "^11.7.0",
    "webextension-polyfill": "^0.10.0"
  }
}
